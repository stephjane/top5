<h1>My Top 5</h1>

  <div class="row">
    <% @songs.each do |song| %>
    <div class="col-md-6">
      <div class="well">
          <h3><%= link_to song.name, song %></h3>
          <h3><%= song.artist.name %></h3>
          <p>
            <% song.rating.times do %>
              <span class=" glyphicon glyphicon-star"></span>
            <%end%>
          </p>         
      </div>
    </div>
    <% end %>
  </div>
